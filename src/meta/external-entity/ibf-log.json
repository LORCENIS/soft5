{
    "name": "ibf-log",
    "version": "0.1",
    "description": "The IBF log data format",
    "dimensions": [
        {
            "name": "N",
            "description": "Number of log points"
        },
        {
            "name": "D",
            "description": "Number of spacial dimensions"
        }
    ],
    "properties": [
        {
            "name": "processor",
            "type": "integer"
        },
        {
            "name": "index",
            "type": "integer",
            "dim": "N"
        },
        {
            "name": "vectorDataDescription",
            "type": "string"
        },
        {
            "name": "scalarDataDescription",
            "type": "string"
        },
        {
            "name": "time",
            "type": "double",
            "unit": "s",
            "dim": "N"
        },
        {
            "name": "vectorData",
            "type": "double",
            "dim": "N,D"
        },
        {
            "name": "scalar",
            "type": "double",
            "dim": "N"
        },
        {
            "name": "position",
            "type": "double",
            "dim": "N,D"
        }
    ]
}
